{
  "navn": "Generer daglig statistikk av medlemsdata for uttrekksdato ${uttreksdato}",
  "metainfo": {
    "mal": "maler/reserveberegning/metaopplaster_grunnlagsdata_daglig_statistikk.json",
    "tags": ["eksport"],
    "st√∏ttetAvPaOrkBa01FraVersjon": "1.4.6"
  },
  "variabler": {
    "uttrekksdato": "<yyyy.MM.dd>",
    "paFakBa12Versjon": "1.0.13",
    "paOrkBa05Versjon": "0.0.8"
  },
  "operasjoner": [
    {
      "handling": "deploy_batch",
      "batcher": [
        {
          "navn": "pa_fak_ba_12",
          "versjon": "${paFakBa12Versjon}"
        },
        {
          "navn": "pa_ork_ba_05",
          "versjon": "${paOrkBa05Versjon}"
        }
      ]
    },
    {
      "handling": "rydd_grunnlagsdata_for_batch",
      "batch": "pa_fak_ba_12"
    },
    {
      "handling": "rydd_grunnlagsdata_for_batch",
      "batch": "pa_ork_ba_05"
    },
    {
      "handling": "kopier_grunnlagsdata_fra_pa_res_ba_02_til_batch",
      "uttrekksdato": "${uttrekksdato}",
      "batch": "pa_fak_ba_12"
    },
    {
      "handling": "pa_fak_ba_12",
      "modus": "aggreger_csv",
      "filnavn": "medlemsdata",
      "utFilnavn": "medlemsdata_statistikk",
      "grupperingsfelter": 2,
      "skalTelleAntallGruppert": true
    },
    {
      "handling": "kopier_filer_fra_batch_til_batch",
      "filPrefix": "medlemsdata_statistikk",
      "fraBatch": "pa_fak_ba_12",
      "tilBatch": "pa_ork_ba_05",
      "kopierFraTidsserieKatalog": false
    },
    {
      "handling": "pa_ork_ba_05",
      "program": "medlemsdata_statistikk",
      "uttrekksdato": "${uttrekksdato}",
      "midlertidig": true
    }
  ]
}