{
  "navn": "Rutine som kopierer filer fra batch ut-mappe eller arkiv til blob storage",
  "variabler": {
    "grunnlagsdataMappe": "#{grunnlagsdatamappeNå}",
    "fraBatch": "<fraBatch>",
    "filPrefix": "<filPrefix>",
    "filstiHosDVH": "<filsti>",
    "nøkkel": "<nøkkel>",
    "grunnlagsdataMappeFraArkiv": "<grunnlagsdataMappeFraArkiv>"
  },
  "operasjoner": [
    {
      "navn": "kopier_filer_fra_batch_til_skyopplaster_og_bestill",
      "handling": "gruppe",
      "gruppeAv": [
        {
          "handling": "kopier_filer_fra_batch_til_skyopplaster_og_bestill",
          "fraBatch": "${fraBatch}",
          "filPrefix": "${filPrefix}",
          "filstiHosDVH": "${filstiHosDVH}"
        }
      ]
    },
    {
      "navn": "kopier_filer_fra_arkiv_til_skyopplaster_og_bestill",
      "handling": "gruppe",
      "gruppeAv": [
        {
          "handling": "kopier_filer_fra_arkiv_til_skyopplaster_og_bestill",
          "fraBatch": "${fraBatch}",
          "filPrefix": "${filPrefix}",
          "nøkkel": "${nøkkel}",
          "filstiHosDVH": "${filstiHosDVH}",
          "grunnlagsdataMappe": "${grunnlagsdataMappeFraArkiv}"
        }
      ]
    }
  ]
}