{
  "navn": "Rutine for kjøring av pa_ork_ba_01 for POA AFP prognosefakturering for termin ${termin} og prognoseversjon ${prognoseVersjon} med grunnlagsdataMappe ${grunnlagsdataMappe}",
  "gyldigVersjon": "0.0.20",
  "variabler": {
    "grunnlagsdataMappe" : "#{grunnlagsdatamappeNå}",
    "termin" : "<termin>",
    "prognoseVersjon" : "<prognoseVersjon>"
  },
  "operasjoner": [
    {
      "handling": "rydd_grunnlagsdata_for_batch",
      "batch": "pa_fak_ba_04",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}"
    },
    {
      "handling": "kopier_filer_fra_aktuar_til_batch",
      "premiemodell": "poa_afp",
      "periode": "${termin}",
      "prognoseVersjon": "${prognoseVersjon}",
      "batch": "pa_fak_ba_04",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}"
    },
    {
      "handling": "last_ned_referanse_data_for_batch",
      "filnavn": "termininfo",
      "batch": "pa_fak_ba_04",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}"
    },

    {
      "handling": "pa_fak_ba_04",
      "modus": "generer_prognosefakturaunderlag_poa_afp",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}",
      "termin": "${termin}",
      "premiemodell": "POA_AFP",
      "validerFeilendeMedlemmer": true
    },
    {
      "handling": "arkiver_batch_output",
      "filnavn": "fakturaunderlag",
      "batch": "pa_fak_ba_04",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}",
      "nøkkel": "generer_prognosefakturaunderlag_poa_afp/${termin}"
    },
    {
      "handling": "pa_fak_ba_04",
      "modus": "generer_agressofil_for_poa_afp_prognose",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}",
      "termin": "${termin}",
      "premiemodell": "POA_AFP",
      "validerFeilendeMedlemmer": true
    },
    {
      "handling": "arkiver_batch_output",
      "filnavn": "agressofil_poa_afp_prognose",
      "batch": "pa_fak_ba_04",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}",
      "nøkkel": "generer_agressofil_for_poa_afp_prognose/${termin}",
      "filter": "Agressofil"
    },


    {
      "handling": "rydd_grunnlagsdata_for_batch",
      "batch": "pa_fak_ba_09",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}"
    },
    {
      "handling": "kopier_filer_fra_aktuar_til_batch",
      "premiemodell": "poa_afp",
      "periode": "${termin}",
      "prognoseVersjon": "${prognoseVersjon}",
      "batch": "pa_fak_ba_09",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}"
    },
    {
      "handling": "last_ned_referanse_data_for_batch",
      "filnavn": "termininfo",
      "batch": "pa_fak_ba_09",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}"
    },

    {
      "handling": "pa_fak_ba_09",
      "modus": "prognosepremier_poa_afp_rapport_avtale",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}",
      "validerFeilendeMedlemmer": true
    },
    {
      "handling": "send_batch_output_til_dvh",
      "filPrefix": "prognosepremier_poa_afp_rapport_avtale",
      "filListe": "POAAFP_FILLISTE_AVTALE.txt",
      "batch": "pa_fak_ba_09",
      "filstiHosDVH": "/poa_afp/fakturering/kjeder/avtale"
    },
    {
      "handling": "arkiver_batch_output",
      "filnavn": "prognosepremier_poa_afp_rapport_avtale",
      "batch": "pa_fak_ba_09",
      "grunnlagsdataMappe": "${grunnlagsdataMappe}",
      "nøkkel": "prognosepremier_poa_afp_rapport_avtale/${termin}"
    }
  ]
}