{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "navn": {
      "type": "string"
    },
    "metainfo": {
      "type": "object",
      "additionalProperties": true,
      "allOf": [
        {
          "required": ["kategori", "mal", "støttetAvPaOrkBa01FraVersjon"],
          "properties": {
            "kategori": {
              "type": "string",
              "enum": ["fakturering", "prognose", "arkivering", "eksport", "systemkorreksjon", "månedskjøring"]
            },
            "premiemodell": {
              "enum": [
                "hbf_fls",
                "hbf_stat",
                "hbf_as_asa",
                "hbf_ovrige",
                "poa_pen",
                "poa_kjeder",
                "poa_frittstaaende",
                "tjenestemannsorg",
                "tjenestemannsorg_arbeidsgiverandel"
              ],
              "type": "string"
            }
          }
        },
        {
          "if": {
            "properties": {
              "kategori": {
                "const": "fakturering"
              }
            },
            "then": {
              "required": ["premiemodell"]
            }
          }
        }
      ]
    },
    "gyldigVersjon": {
      "type": "string"
    },
    "variabler": {
      "type": "object",
      "additionalProperties": {
        "type": "string"
      }
    },
    "operasjoner": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "object",
        "properties": {
          "handling": {
            "type": "string"
          }
        },
        "required": ["handling"],
        "additionalProperties": true
      }
    }
  },
  "required": ["navn", "metainfo", "variabler", "operasjoner"],
  "additionalProperties": false
}